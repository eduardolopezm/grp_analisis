	<queryString>
		<![CDATA[Select typename, debtortrans.type, debtortrans.transno, origtrandate, invtext, date_format(origtrandate,'%d/%m/%Y') as fecha,
	if(ovamount >0,(ovamount+ovgst),0) as cargos,
	if(ovamount < 0,-1*(ovamount+ovgst),0) as abonos, debtortrans.folio, tags.legalid, lb.legalname as nom_razon, debtortrans.tagref,
	tags.tagdescription as nom_negocio, d.u_department, d.department as nom_depa,
	(debtortrans.ovamount + debtortrans.ovgst + debtortrans.ovfreight + debtortrans.ovdiscount-alloc) as saldo1,
	ABS((debtortrans.ovamount+debtortrans.ovgst+debtortrans.ovfreight+debtortrans.ovdiscount)-alloc) as saldo2,
	Case When sm.narrative Is Null Then "" Else sm.narrative End as narrative, debtortrans.debtorno, dm.name as nom_cliente, tags.areacode, a.areadescription as nom_area
FROM debtortrans
LEFT JOIN systypescat ON debtortrans.type = systypescat.typeid
left join tags on tags.tagref=debtortrans.tagref
LEFT JOIN areas a ON tags.areacode = a.areacode
LEFT JOIN departments d ON tags.u_department = d.u_department
Left Join stockmoves sm On debtortrans.transno= sm.transno and debtortrans.type= sm.type
Left Join legalbusinessunit lb On tags.legalid= lb.legalid
Left Join debtorsmaster dm On debtortrans.debtorno= dm.debtorno
WHERE debtortrans.origtrandate BETWEEN $P{fechaini} and $P{fechafin}
	and debtortrans.debtorno = $P{num_cliente}
	and debtortrans.ovamount <> 0
	AND (Case When $P{unidadneg} Is Not Null and $P{unidadneg}!='0' Then debtortrans.tagref= $P{unidadneg}
		Else debtortrans.tagref In (select tagref from sec_unegsxuser where userid= $P{userid}) End)
	And tags.areacode = Case When $P{areacode} Is Not Null and $P{areacode}!='0' Then $P{areacode} Else tags.areacode End
	And tags.legalid = Case When $P{razonsocial} Is Not Null And $P{razonsocial}!= '0' Then $P{razonsocial} Else tags.legalid End
	And d.u_department = Case When $P{departamento} Is Not Null and $P{departamento}!= '0' Then $P{departamento} Else d.u_department End
Order By debtortrans.origtrandate]]>
	</queryString>
