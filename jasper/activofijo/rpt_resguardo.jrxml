	<queryString>
		<![CDATA[select distinct        @rownum := @rownum + 1 AS rank, a.assetid, folio, fecha, b.barcode, cabm, c.realname, b.marca, b.model as modelo, b.serialno, b.cost, description, longdescription from  fixedasset_Resguardos a left outer join
fixedassets b on a.assetid = b.assetid
join www_users c on c.userid = a.userid
,
       (SELECT @rownum := 0) r
where folio = $P{folioresguardo}
order by rank, assetid]]>
