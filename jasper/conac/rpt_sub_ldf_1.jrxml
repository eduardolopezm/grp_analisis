	<parameter name="cuenta_mayor" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["1.1"]]></defaultValueExpression>
	</parameter>
	<parameter name="titulo_reporte" class="java.lang.String" isForPrompting="false">
